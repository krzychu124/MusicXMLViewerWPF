<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MusicXMLScore.ViewModel"
             xmlns:helpers="clr-namespace:MusicXMLScore.Helpers"
             xmlns:credit ="clr-namespace:MusicXMLViewerWPF"
             xmlns:Configuration="clr-namespace:MusicXMLScore.ViewModel" xmlns:Log="clr-namespace:MusicXMLScore.Log" x:Class="MusicXMLScore.ViewModel.PageView"
             mc:Ignorable="d" Height="{Binding PageHeight}" Width="{Binding PageWidth}" x:Name="uc" d:DesignWidth="500" d:DesignHeight="250.333">

    <Grid x:Name="gridPage" Width="Auto" Height="Auto" Background="Beige" >
        <ItemsControl x:Name="PartsSegments" ItemsSource="{Binding PartsSegments}">
            <ItemsControl.ItemsPanel>
                <ItemsPanelTemplate>
                    <!--<helpers:CustomPartsSegmentPanel IsHitTestVisible="False" Height="Auto"/>-->
                    <VirtualizingStackPanel></VirtualizingStackPanel>
                </ItemsPanelTemplate>
            </ItemsControl.ItemsPanel>
        </ItemsControl>
    </Grid>
</UserControl>
